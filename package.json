    "scripts": {
        "build": "webpack && babel ./src/server.js --out-file ./dist/server.js",
        "build-dev": "cross-env NODE_ENV=dev webpack && babel ./src/server.js --out-file ./dist/server.js",
        "build-prod": "cross-env NODE_ENV=prod webpack && babel ./src/server.js --out-file ./dist/server.js",
        "build-rollup": "rollup --config build/rollup.config.js",
        "start": "node ./dist/server.js",
        "dev": "cross-env NODE_ENV=dev node ./dist/server.js",
        "debug": "babel-node ./dist/server.js --inspect-brk=localhost:8080"
    },
